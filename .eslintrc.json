{
    "env": {
        "es6": true
    },
    "extends": [
        "standard",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "unused-imports",
        "@typescript-eslint"
    ],
    "rules": {
        "unused-imports/no-unused-imports": "error"
    }
}